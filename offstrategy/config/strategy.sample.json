{
    "name": "Default",
    "description": "Default provisioning strategy",
    "version": "0.0.1",
    "node": {
        "image": {
            "options": [
                {
                    "name": "CoreOS-stable-717.1.0",
                    "provider": "EC2_AP_SOUTHEAST2",
                    "region": "EC2_AP_SOUTHEAST2",
                    "driver": "EC2APSESydneyNodeDriver"
                },
                {
                    "name": "Ubuntu Linux 14.04 LTS Trusty Tahr - Minimal Install (64 bit)",
                    "provider": "SOFTLAYER",
                    "region": "syd01",
                    "driver": "SoftLayerNodeDriver"
                },
                {
                    "name": "Ubuntu - Latest (64 bit)",
                    "provider": "SOFTLAYER",
                    "region": "syd01",
                    "driver": "SoftLayerNodeDriver"
                },
                {
                    "name": "Ubuntu - Latest",
                    "provider": "SOFTLAYER",
                    "region": "syd01",
                    "driver": "SoftLayerNodeDriver"
                }
            ],
            "pick": "first"
        },
        "hardware": {
            "options": [
                {
                    "id": "t1.micro",
                    "name": "Micro Instance",
                    "provider": "EC2_AP_SOUTHEAST2",
                    "region": "EC2_AP_SOUTHEAST2",
                    "driver": "EC2APSESydneyNodeDriver"
                },
                {
                    "name": "1 CPU, 1GB ram, 25GB",
                    "provider": "SOFTLAYER",
                    "region": "syd01",
                    "driver": "SoftLayerNodeDriver"
                },
                {
                    "name": "4 CPU, 8GB ram, 100GB",
                    "provider": "SOFTLAYER",
                    "region": "syd01",
                    "driver": "SoftLayerNodeDriver"
                }
            ]
        },
        "location": {
            "options": [
                {
                    "id": "0",
                    "region": "ap-southeast-2a",
                    "provider": "EC2_AP_SOUTHEAST2",
                    "driver": "EC2APSESydneyNodeDriver"
                },
                {
                    "id": "1",
                    "region": "ap-southeast-2b",
                    "provider": "EC2_AP_SOUTHEAST1",
                    "driver": null
                },
                {
                    "id": "syd01",
                    "region": "syd01",
                    "provider": "SOFTLAYER",
                    "driver": "SoftLayerNodeDriver"
                }
            ]
        }
    },
    "provider": {
        "options": [
            {
                "EC2_AP_SOUTHEAST2": {
                    "auth": {
                        "username": "${env.AWS_KEY_ID}",
                        "key": "${env.AWS_ACCESS_KEY}"
                    },
                    "ssh": {
                        "public_key_path": "${env.PUBLIC_KEY_PATH}",
                        "private_key_path": "${env.PRIVATE_KEY_PATH}",
                        "key_name": "${env.PUBLIC_KEY_NAME}"
                    },
                    "security_group": [
                        "all"
                    ],
                    "driver": "EC2APSESydneyNodeDriver",
                    "key_name": "${env.PUBLIC_KEY_NAME}"
                }
            },
            {
                "SOFTLAYER": {
                    "auth": {
                        "username": "${env.SL_USER}",
                        "key": "${env.SL_KEY}"
                    },
                    "ssh": {
                        "public_key_path": "${env.PUBLIC_KEY_PATH}",
                        "private_key_path": "${env.PRIVATE_KEY_PATH}",
                        "key_name": "${env.PUBLIC_KEY_NAME}"
                    },
                    "driver": "SoftLayerNodeDriver"
                }
            }
        ]
    },
    "purpose": [
        "any-cluster"
    ],
    "etcd_server": "http://localhost:2379",
    "default_pick": "first"
}