{
    "name": "Default",
    "description": "AWS ubuntu provisioning strategy",
    "version": "0.0.1",
    "node": {
        "image": {
            "options": [
                {
                    "name": "ubuntu/images-testing/ebs/ubuntu-trusty-daily-amd64-server-20150708",
                    "provider": "EC2_AP_SOUTHEAST2",
                    "region": "EC2_AP_SOUTHEAST2",
                    "driver": "EC2APSESydneyNodeDriver"
                }
            ],
            "pick": "first"
        },
        "hardware": {
            "options": [
                {
                    "id": "t1.micro",
                    "name": "Micro Instance",
                    "provider": "EC2_AP_SOUTHEAST2",
                    "region": "EC2_AP_SOUTHEAST2",
                    "driver": "EC2APSESydneyNodeDriver"
                }
            ]
        },
        "location": {
            "options": [
                {
                    "id": "0",
                    "region": "ap-southeast-2a",
                    "provider": "EC2_AP_SOUTHEAST2",
                    "driver": "EC2APSESydneyNodeDriver"
                }
            ]
        }
    },
    "provider": {
        "options": [
            {
                "EC2_AP_SOUTHEAST2": {
                    "auth": {
                        "username": "${env.AWS_KEY_ID}",
                        "key": "${env.AWS_ACCESS_KEY}"
                    },
                    "ssh": {
                        "public_key_path": "${env.PUBLIC_KEY_PATH}",
                        "private_key_path": "${env.PRIVATE_KEY_PATH}",
                        "key_name": "${env.PUBLIC_KEY_NAME}"
                    },
                    "security_group": [
                        "all"
                    ],
                    "driver": "EC2APSESydneyNodeDriver",
                    "key_name": "${env.PUBLIC_KEY_NAME}"
                }
            }
        ]
    },
    "purpose": [
        "any-cluster"
    ],
    "etcd_server": "http://localhost:2379",
    "default_pick": "first"
}