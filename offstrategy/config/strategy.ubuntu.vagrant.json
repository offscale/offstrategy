{
  "name": "Vagrant example",
  "description": "Example with Vagrant",
  "version": "0.0.1",
  "node": {
    "image": {
      "options": [
        {
          "id": "ubuntu1604-bighd",
          "provider": {
            "name": "VAGRANT",
            "region": null
          }
        },
        {
          "id": "ubuntu/xenial64",
          "provider": {
            "name": "VAGRANT",
            "region": null
          }
        }
      ],
      "pick": "first"
    },
    "hardware": {
      "options": [
        {
          "ram": 8192,
          "disk": 30,
          "extra": {
            "cpus": 4
          },
          "provider": {
            "name": "VAGRANT",
            "visor": "virtualbox",
            "region": null
          }
        }
      ]
    },
    "location": {
      "options": [
        {
          "provider": {
            "id": null,
            "name": "VAGRANT",
            "region": null
          }
        }
      ]
    },
    "extras": {
      "options": [
        {
          "first_lines": [
            "config.vm.network :private_network, ip: '192.168.33.10'",
            "config.hostsupdater.aliases = ['preview.localhost']",
            ""
          ],
          "blocks": [
            {
              "name": "config.vm.provider",
              "args": "v",
              "func_args": "\"virtualbox\"",
              "last_lines": [
                "v.customize ['modifyvm', :id, '--natdnshostresolver1', 'on']",
                "v.customize ['modifyvm', :id, '--natdnsproxy1', 'on']",
                "v.cpus = 4",
                "v.memory = 8192"
              ]
            }
          ],
          "provider": {
            "name": "VAGRANT",
            "region": null
          }
        }
      ]
    }
  },
  "provider": {
    "options": [
      {
        "provider": {
          "name": "VAGRANT",
          "key": "/mnt/large_linux/vagrant/openedx-ficus.master/Vagrantfile",
          "region": null
        },
        "auth": {
          "ex_vagrantfile": "/mnt/large_linux/vagrant/openedx-ficus.master/Vagrantfile",
          "key": null
        }
      }
    ]
  },
  "purpose": [
    "any-cluster"
  ],
  "etcd_server": "http://localhost:2379",
  "default_pick": "first"
}
