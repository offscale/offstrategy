{
  "name": "Vagrant example",
  "description": "Example with Vagrant",
  "version": "0.0.1",
  "node": {
    "image": {
      "options": [
        {
          "name": "Ubuntu Linux 14.04 LTS Trusty Tahr - Minimal Install (64 bit)",
          "provider": {
            "name": "SOFTLAYER",
            "region": "syd01"
          }
        }
      ],
      "pick": "first"
    },
    "hardware": {
      "options": [
        {
          "ram": 7168,
          "name": "Cool specs",
          "extra": {
            "cores": 2,
            "max_data_disks": 4
          },
          "disk": 127,
          "provider": {
            "name": "VAGRANT",
            "region": null
          }
        }
      ]
    },
    "location": {
      "options": [
        {
          "provider": {
            "id": null,
            "name": "VAGRANT",
            "region": null
          }
        }
      ]
    }
  },
  "provider": {
    "options": [
      {
        "provider": {
          "name": "VAGRANT",
          "key": "/mnt/large_linux/vagrant/edx-fullstack/Vagrantfile",
          "region": null
        },
        "auth": {
          "ex_vagrantfile": "/mnt/large_linux/vagrant/edx-fullstack/Vagrantfile",
          "key": null
        }
      }
    ]
  },
  "purpose": [
    "any-cluster"
  ],
  "etcd_server": "http://localhost:2379",
  "default_pick": "first"
}
